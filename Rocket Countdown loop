// Declare variables
// This is the timer
var timeleft = 10;
// repeats the timer
var downloadTimer = setInterval(function(){
  if(timeleft <= 0){
    timeleft = 10;
  }
    // seconds remaining note 
  {
    document.getElementById("countdown").innerHTML = timeleft + " seconds remaining";
  }
  timeleft -= 1;
}, 1000);
    </script>
